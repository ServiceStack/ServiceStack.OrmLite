<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>ServiceStack.OrmLite.Oracle</id> 
    <title>OrmLite.Oracle - Fast, code-first, config-free Poco ORM</title>
    <version>5.0.0</version>
    <authors>ServiceStack, Guru Kathiresan</authors> 
    <owners>ServiceStack, Guru Kathiresan</owners>
    <description>
      Light, simple and fast convention-based code-first POCO ORM for Oracle RDBMS (Unofficial).
      Support for Creating and Dropping Table Schemas from POCOs, Complex Property types transparently 
      stored in schemaless text blobs in Oracle.
    </description> 
    <projectUrl>https://github.com/ServiceStack/ServiceStack.OrmLite</projectUrl>
    <licenseUrl>https://servicestack.net/terms</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <tags>Oracle SQL OrmLite POCO Code-First Orm Schema-less Blobs</tags>
    <language>en-US</language> 
    <copyright>ServiceStack and contributors</copyright>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="dotConnect.Express.for.Oracle" version="8.3.115" />
        <dependency id="odp.net.x64" version="112.3.20" />
        <dependency id="odp.net.x86" version="112.3.20" />
        <dependency id="ServiceStack.OrmLite" version="5.0.0" />
      </group>
    </dependencies>
  </metadata> 
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack.OrmLite.Oracle\**\*.cs" target="src\ServiceStack.OrmLite.Oracle" />
  </files>
</package>

<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>ServiceStack.OrmLite.Firebird</id>
    <title>OrmLite.FireBird - Fast, code-first, config-free Poco ORM</title>
    <version>4.5.0</version>
    <authors>Service Stack, Angel Colmenares</authors> 
    <owners>Service Stack, Angel Colmenares</owners>
    <description>
        Light, simple and fast convention-based code-first POCO ORM for Firebird. 
		Support for Creating and Dropping Table Schemas from POCOs, Complex Property types transparently stored in schemaless text blobs in Firebird.
    </description> 
    <projectUrl>https://github.com/ServiceStack/ServiceStack.OrmLite</projectUrl>
    <licenseUrl>https://servicestack.net/terms</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <tags>Firebird OrmLite POCO Code-First Orm Schema-less Blobs</tags>
    <language>en-US</language> 
    <copyright>Copyright 2017 ServiceStack</copyright>
    <dependencies>
      <dependency id="ServiceStack.OrmLite" version="4.5" />
      <dependency id="FirebirdSql.Data.FirebirdClient" version="4.10.0.0" />
    </dependencies> 
  </metadata> 
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack.OrmLite.Firebird\**\*.cs" target="src\ServiceStack.OrmLite.Firebird" />
  </files>
</package>

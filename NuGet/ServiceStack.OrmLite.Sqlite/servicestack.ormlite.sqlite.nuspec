    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <tags>Sqlite OrmLite POCO Code-First ORM Schema-less Blobs Windows</tags>
    <language>en-US</language> 
    <copyright>Copyright 2017 ServiceStack</copyright>
    <dependencies>
      <dependency id="ServiceStack.OrmLite" version="4.5" />
      <dependency id="System.Data.SQLite.Core" version="1.0.105.2" />
    </dependencies> 
  </metadata> 
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\ServiceStack.OrmLite.Sqlite\**\*.cs" target="src\ServiceStack.OrmLite.Sqlite" />
  </files>
</package>
